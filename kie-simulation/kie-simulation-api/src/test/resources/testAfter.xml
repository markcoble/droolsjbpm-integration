<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<executable-fluent>
    <batch distance="1000">
        <new-application-context name="app1"/>
        <get-kie-container>
            <releaseId xsi:type="afReleaseIdImpl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <artifactId>artifact</artifactId>
                <groupId>org.default</groupId>
                <version>1.0.0-SNAPSHOT</version>
            </releaseId>
        </get-kie-container>
        <new-session/>
        <insert return-object="true" entry-point="DEFAULT" disconnected="false">
            <person>
                <addressArray>
                    <number>l2</number>
                    <phone>l3</phone>
                    <street>add1</street>
                </addressArray>
                <addressArray>
                    <number>l2</number>
                    <phone>l3</phone>
                    <street>add2</street>
                </addressArray>
                <age>0</age>
                <name>Simmy Sim</name>
            </person>
        </insert>
        <fire-all-rules max="-1"/>
        <get-global identifier="person"/>
        <out name="outS1"/>
        <get-global identifier="timeNow"/>
        <out name="timeNow1"/>
        <dispose/>
    </batch>
